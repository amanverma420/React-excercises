//Not optimised code

#include<stdlib.h>
#include<iostream>
#include<stdio.h>
using namespace std;
int main(){
    int arr[]={5,0,1,2,0,0,4,0,3};
    int n=sizeof(arr)/sizeof(arr[0]);

    //print unsorted array 

    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }

    for(int i=0;i<n-1;i++){  //n-1 passes
        //traverse 
        for(int j=0;j<n-1;j++){ 
            if(arr[j]==0){ //swap
                // int temp=arr[j];
                // arr[j]=arr[j+1];
                // arr[j+1]=temp;
                swap(arr[j],arr[j+1]);
            }
        }
    }
    //print sorted array 
    cout<<endl;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
}
