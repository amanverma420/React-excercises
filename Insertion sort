#include <iostream>
using namespace std;

int main() {
    int arr[] = {253, 5, 14, 75, 21, 65, 43};
    int n = sizeof(arr) / sizeof(arr[0]);

    // Print original array
    cout << "Original array: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    // Insertion Sort algorithm
    for (int i = 1; i < n; i++) {
        int j = i;
        while (j >= 1) {
            if (arr[j] >= arr[j - 1]) break;
            else {
                swap(arr[j], arr[j - 1]);
                j--;
            }
        }
    }

    // Print sorted array
    cout << "Sorted array: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
