#include<iostream>
using namespace std;
int fact(int n){
    int fact=1;
    for(int i=n;i>0;i--){
        fact=fact*i;
    }
    return fact;
}
int combination(int n,int r){
    int x=fact(n)/(fact(r)*fact(n-r));
    return x;

}
int main()
{
    int n;
    cin>>n;
    for(int i=0;i<=n;i++){
        for(int j=0;j<=i;j++){
            cout<<combination(i,j)<<" ";
        }
        cout<<endl;
    }
}
